<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane maxWidth="-Infinity" minWidth="-Infinity" prefWidth="720.0" vgap="5.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="-Infinity" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <GridPane GridPane.rowIndex="1">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
            <RowConstraints minHeight="-Infinity" vgrow="ALWAYS" />
            <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <TableView prefWidth="690.0" GridPane.rowIndex="1">
               <columns>
                  <TableColumn maxWidth="-Infinity" minWidth="120.0" prefWidth="120.0" resizable="false" text="Quiz Name" />
                  <TableColumn maxWidth="-Infinity" minWidth="160.0" prefWidth="160.0" resizable="false" text="School" />
                  <TableColumn maxWidth="160.0" minWidth="160.0" prefWidth="160.0" text="Question" />
                  <TableColumn maxWidth="-Infinity" minWidth="75.0" prefWidth="75.0" text="Correct (%)" />
                  <TableColumn maxWidth="-Infinity" minWidth="85.0" prefWidth="85.0" text="Incorrect (%)" />
                  <TableColumn maxWidth="80.0" minWidth="80.0" text="given up (n)" />
               </columns>
               <GridPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </GridPane.margin>
            </TableView>
            <Label text="Summary" GridPane.halignment="CENTER" />
            <Button mnemonicParsing="false" text="Export" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
               <GridPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </GridPane.margin>
            </Button>
         </children>
      </GridPane>
      <GridPane hgap="5.0">
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="-Infinity" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <HBox spacing="5.0">
               <children>
                  <Label text="Current Quiz :" />
                  <Label fx:id="quizNameLabel" />
               </children>
            </HBox>
            <HBox prefWidth="200.0" spacing="5.0" GridPane.columnIndex="1">
               <children>
                  <Label text="Date :" textAlignment="RIGHT" />
                  <Label fx:id="quizDateLabel" />
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#showQuizSetupDialog" text="Setup" GridPane.columnIndex="2" GridPane.halignment="RIGHT" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </GridPane>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</GridPane>
